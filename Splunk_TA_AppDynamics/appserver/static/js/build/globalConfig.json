                ]
            }, 
            "description": "Manage the data inputs", 
            "services": [
                {
                    "entity": [
                        {
                            "required": true, 
                            "field": "name", 
                            "type": "text", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }, 
                                {
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100, 
                                    "minLength": 1
                                }
                            ], 
                            "label": "Name"
                        }, 
                        {
                            "help": "Time interval of input in seconds.", 
                            "field": "interval", 
                            "type": "text", 
                            "label": "Interval", 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ]
                        }, 
                        {
                            "defaultValue": "default", 
                            "field": "index", 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "required": true, 
                            "options": {
                                "blackList": "^_.*$", 
                                "endpointUrl": "data/indexes", 
                                "createSearchChoice": true
                            }, 
                            "validators": [
                                {
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80, 
                                    "minLength": 1
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "host_port", 
                            "type": "text", 
                            "help": "Enter the host and port for your AppDynamics Collector", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "AppDynamics Host:Port"
                        }, 
                        {
                            "required": true, 
                            "field": "auth_token", 
                            "type": "text", 
                            "help": "For a single tenant controller use the output of.: \necho -n '<user>@customer1:<password>' | base64'\n\nfor multi-tenant controller use the output of:\necho -n '<user>@<accountname>:<password>' | base64'\n", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "Authorization Token"
                        }, 
                        {
                            "defaultValue": "5", 
                            "field": "duration", 
                            "type": "text", 
                            "help": "The time period (in minutes) that you wish to retrieve data for.  \ne.g.:  5 = retrieve data for the past 5 minutes.", 
                            "required": true, 
                            "label": "Tim Duration (in minutes)", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "app_name", 
                            "type": "text", 
                            "help": "The name (or ID) of your Application in AppDynamics", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "Application Name"
                        }, 
                        {
                            "defaultValue": "/metric-data", 
                            "field": "url_path", 
                            "type": "text", 
                            "help": "e.g.:  /metric-data    --or-- /events   --or--  /problems/healthrule-violations", 
                            "required": true, 
                            "label": "URL Path", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "defaultValue": "metric-path=Overall Application Performance|*", 
                            "field": "params", 
                            "type": "text", 
                            "help": "e.g.: /metric-data  REQUIRES  metric-path=Overall Application Performance|* (hint: use 'Copy Full Path' in the AppDynamics Metric Browser)", 
                            "required": true, 
                            "label": "Parameters", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ]
                        }
                    ], 
                    "title": "AppDynamics Single API Call", 
                    "name": "appdynamics_single_api"
                }, 
                {
                    "entity": [
                        {
                            "required": true, 
                            "field": "name", 
                            "type": "text", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }, 
                                {
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100, 
                                    "minLength": 1
                                }
                            ], 
                            "label": "Name"
                        }, 
                        {
                            "help": "Time interval of input in seconds.", 
                            "field": "interval", 
                            "type": "text", 
                            "label": "Interval", 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ]
                        }, 
                        {
                            "defaultValue": "default", 
                            "field": "index", 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "required": true, 
                            "options": {
                                "blackList": "^_.*$", 
                                "endpointUrl": "data/indexes", 
                                "createSearchChoice": true
                            }, 
                            "validators": [
                                {
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80, 
                                    "minLength": 1
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "host_port", 
                            "type": "text", 
                            "help": "Enter the URL and Port for your AppDynamics Collector", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "AppD Collector URL"
                        }, 
                        {
                            "required": true, 
                            "field": "auth_token", 
                            "type": "text", 
                            "help": "For a single tenant controller use the output of.: \necho -n '<user>@customer1:<password>' | base64'\n\nfor multi-tenant controller use the output of:\necho -n '<user>@<accountname>:<password>' | base64'\n", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "Authorization Token"
                        }, 
                        {
                            "defaultValue": "5", 
                            "field": "duration", 
                            "type": "text", 
                            "help": "The time period (in minutes) that you wish to retrieve data for.  e.g.:  5 = retrieve data for the past 5 minutes.", 
                            "required": true, 
                            "label": "Time Duration (in minutes)", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "required": false, 
                            "field": "app_name", 
                            "type": "text", 
                            "help": "Leave this blank to retrieve data for ALL applications in AppDynamics.  If you only want a single application, enter that application's name here.", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "label": "Application Name (optional)"
                        }
                    ], 
                    "title": "AppDynamics Summary", 
                    "name": "appdynamics_summary"
                }
            ], 
            "title": "Inputs"
        }, 
        "configuration": {
            "description": "Setup your add-on global configurations", 
            "tabs": [
                {
                    "entity": [
                        {
                            "field": "proxy_enabled", 
                            "type": "checkbox", 
                            "label": "Enable"
                        }, 
                        {
                            "defaultValue": "http", 
                            "field": "proxy_type", 
                            "type": "singleSelect", 
                            "label": "Proxy Type", 
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "value": "http", 
                                        "label": "http"
                                    }, 
                                    {
                                        "value": "socks4", 
                                        "label": "socks4"
                                    }, 
                                    {
                                        "value": "socks5", 
                                        "label": "socks5"
                                    }
                                ]
                            }
                        }, 
                        {
                            "field": "proxy_url", 
                            "type": "text", 
                            "label": "Host", 
                            "validators": [
                                {
                                    "errorMsg": "Max host length is 4096", 
                                    "type": "string", 
                                    "maxLength": 4096, 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_port", 
                            "type": "text", 
                            "label": "Port", 
                            "validators": [
                                {
                                    "type": "number", 
                                    "range": [
                                        1, 
                                        65535
                                    ]
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_username", 
                            "type": "text", 
                            "label": "Username", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of username is 50", 
                                    "type": "string", 
                                    "maxLength": 50, 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_password", 
                            "type": "text", 
                            "label": "Password", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "encrypted": true
                        }, 
                        {
                            "field": "proxy_rdns", 
                            "type": "checkbox", 
                            "label": "Reverse DNS resolution"
                        }
                    ], 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "title": "Proxy", 
                    "name": "proxy"
                }, 
                {
                    "entity": [
                        {
                            "defaultValue": "INFO", 
                            "field": "loglevel", 
                            "type": "singleSelect", 
                            "label": "Log level", 
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG", 
                                        "label": "DEBUG"
                                    }, 
                                    {
                                        "value": "INFO", 
                                        "label": "INFO"
                                    }, 
                                    {
                                        "value": "WARNING", 
                                        "label": "WARNING"
                                    }, 
                                    {
                                        "value": "ERROR", 
                                        "label": "ERROR"
                                    }, 
                                    {
                                        "value": "CRITICAL", 
                                        "label": "CRITICAL"
                                    }
                                ]
                            }
                        }
                    ], 
                    "title": "Logging", 
                    "name": "logging"
                }
            ], 
            "title": "Configuration"
        }
    }, 
