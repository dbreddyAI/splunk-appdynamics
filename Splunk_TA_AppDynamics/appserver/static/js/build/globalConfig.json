{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "entity": [
                        {
                            "label": "Enable", 
                            "field": "proxy_enabled", 
                            "type": "checkbox"
                        }, 
                        {
                            "label": "Proxy Type", 
                            "field": "proxy_type", 
                            "defaultValue": "http", 
                            "type": "singleSelect", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "http", 
                                        "label": "http"
                                    }, 
                                    {
                                        "value": "socks4", 
                                        "label": "socks4"
                                    }, 
                                    {
                                        "value": "socks5", 
                                        "label": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }
                        }, 
                        {
                            "label": "Host", 
                            "field": "proxy_url", 
                            "validators": [
                                {
                                    "errorMsg": "Max host length is 4096", 
                                    "minLength": 0, 
                                    "maxLength": 4096, 
                                    "type": "string"
                                }
                            ], 
                            "type": "text"
                        }, 
                        {
                            "label": "Port", 
                            "field": "proxy_port", 
                            "validators": [
                                {
                                    "range": [
                                        1, 
                                        65535
                                    ], 
                                    "type": "number"
                                }
                            ], 
                            "type": "text"
                        }, 
                        {
                            "label": "Username", 
                            "field": "proxy_username", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of username is 50", 
                                    "minLength": 0, 
                                    "maxLength": 50, 
                                    "type": "string"
                                }
                            ], 
                            "type": "text"
                        }, 
                        {
                            "label": "Password", 
                            "field": "proxy_password", 
                            "encrypted": true, 
                            "type": "text", 
                            "validators": [
                                {
                                    "errorMsg": "Max length of password is 8192", 
                                    "minLength": 0, 
                                    "maxLength": 8192, 
                                    "type": "string"
                                }
                            ]
                        }, 
                        {
                            "label": "Reverse DNS resolution", 
                            "field": "proxy_rdns", 
                            "type": "checkbox"
                        }
                    ], 
                    "name": "proxy", 
                    "title": "Proxy", 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }
                }, 
                {
                    "entity": [
                        {
                            "label": "Log level", 
                            "field": "loglevel", 
                            "defaultValue": "INFO", 
                            "type": "singleSelect", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG", 
                                        "label": "DEBUG"
                                    }, 
                                    {
                                        "value": "INFO", 
                                        "label": "INFO"
                                    }, 
                                    {
                                        "value": "WARNING", 
                                        "label": "WARNING"
                                    }, 
                                    {
                                        "value": "ERROR", 
                                        "label": "ERROR"
                                    }, 
                                    {
                                        "value": "CRITICAL", 
                                        "label": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }
                        }
                    ], 
                    "name": "logging", 
                    "title": "Logging"
                }
            ], 
            "description": "Setup your add-on global configurations", 
            "title": "Configuration"
        }, 
