{
    "pages": {
        "configuration": {
            "description": "Set up your add-on", 
            "tabs": [
                {
                    "name": "proxy", 
                    "entity": [
                        {
                            "field": "proxy_enabled", 
                            "label": "Enable", 
                            "type": "checkbox"
                        }, 
                        {
                            "field": "proxy_type", 
                            "label": "Proxy Type", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "defaultValue": "http"
                        }, 
                        {
                            "field": "proxy_url", 
                            "label": "Host", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max host length is 4096", 
                                    "type": "string", 
                                    "maxLength": 4096
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_port", 
                            "label": "Port", 
                            "type": "text", 
                            "validators": [
                                {
                                    "type": "number", 
                                    "range": [
                                        1, 
                                        65535
                                    ]
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_username", 
                            "label": "Username", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of username is 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_password", 
                            "encrypted": true, 
                            "label": "Password", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "field": "proxy_rdns", 
                            "label": "Reverse DNS resolution", 
                            "type": "checkbox"
                        }
                    ], 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "title": "Proxy"
                }, 
                {
                    "name": "logging", 
                    "entity": [
                        {
                            "field": "loglevel", 
                            "label": "Log level", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "defaultValue": "INFO"
                        }
                    ], 
                    "title": "Logging"
                }
            ], 
            "title": "Configuration"
        }, 
        "inputs": {
            "description": "Manage your data inputs", 
            "table": {
                "header": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }
                ], 
                "moreInfo": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }, 
                    {
                        "field": "host_port", 
                        "label": "AppDynamics Host:Port"
                    }, 
                    {
                        "field": "auth_token", 
                        "label": "Authorization Token"
                    }, 
                    {
                        "field": "duration", 
                        "label": "Tim Duration (in minutes)"
                    }, 
                    {
                        "field": "app_name", 
                        "label": "Application Name"
                    }, 
                    {
                        "field": "url_path", 
                        "label": "URL Path"
                    }, 
                    {
                        "field": "params", 
                        "label": "Parameters"
                    }, 
                    {
                        "field": "appd_userid", 
                        "label": "AppD UserID"
                    }, 
                    {
                        "field": "appd_password", 
                        "label": "AppD Password"
                    }, 
                    {
                        "field": "account_name", 
                        "label": "AppD Account Name"
                    }, 
                    {
                        "field": "metrics_to_collect", 
                        "label": "Metrics to Collect"
                    }
                ], 
                "actions": [
                    "edit", 
                    "enable", 
                    "delete", 
                    "clone"
                ]
            }, 
            "services": [
                {
                    "name": "appdynamics_single_api", 
                    "entity": [
                        {
                            "required": true, 
                            "field": "name", 
                            "label": "Name", 
                            "help": "Enter a unique name for the data input", 
                            "type": "text", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "interval", 
                            "label": "Interval", 
                            "help": "Time interval of input in seconds.", 
                            "type": "text", 
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "options": {
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes", 
                                "blackList": "^_.*$"
                            }, 
                            "required": true, 
                            "field": "index", 
                            "label": "Index", 
                            "type": "singleSelect", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ], 
                            "defaultValue": "default"
                        }, 
                        {
                            "required": true, 
                            "field": "host_port", 
                            "label": "AppDynamics Host:Port", 
                            "help": "Enter the host and port for your AppDynamics Collector", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "auth_token", 
                            "label": "Authorization Token", 
                            "help": "For a single tenant controller use the output of.: \necho -n '<user>@customer1:<password>' | base64'\n\nfor multi-tenant controller use the output of:\necho -n '<user>@<accountname>:<password>' | base64'\n", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "duration", 
                            "label": "Tim Duration (in minutes)", 
                            "help": "The time period (in minutes) that you wish to retrieve data for.  \ne.g.:  5 = retrieve data for the past 5 minutes.", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "defaultValue": "5"
                        }, 
                        {
                            "required": true, 
                            "field": "app_name", 
                            "label": "Application Name", 
                            "help": "The name (or ID) of your Application in AppDynamics", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "url_path", 
                            "label": "URL Path", 
                            "help": "e.g.:  /metric-data    --or-- /events   --or--  /problems/healthrule-violations", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "defaultValue": "/metric-data"
                        }, 
                        {
                            "required": true, 
                            "field": "params", 
                            "label": "Parameters", 
                            "help": "e.g.: /metric-data  REQUIRES  metric-path=Overall Application Performance|* (hint: use 'Copy Full Path' in the AppDynamics Metric Browser)", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "defaultValue": "metric-path=Overall Application Performance|*"
                        }
                    ], 
                    "title": "AppDynamics Single API Call"
                }, 
                {
                    "name": "appdynamics_summary", 
                    "entity": [
                        {
                            "required": true, 
                            "field": "name", 
                            "label": "Name", 
                            "help": "Enter a unique name for the data input", 
                            "type": "text", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "interval", 
                            "label": "Interval", 
                            "help": "Time interval of input in seconds.", 
                            "type": "text", 
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "options": {
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes", 
                                "blackList": "^_.*$"
                            }, 
                            "required": true, 
                            "field": "index", 
                            "label": "Index", 
                            "type": "singleSelect", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ], 
                            "defaultValue": "default"
                        }, 
                        {
                            "required": true, 
                            "field": "host_port", 
                            "label": "AppD Collector URL", 
                            "help": "Enter the URL and Port for your AppDynamics Collector", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "appd_userid", 
                            "label": "AppD UserID", 
                            "help": "The userid you use to login to AppDynamics", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "encrypted": true, 
                            "required": true, 
                            "field": "appd_password", 
                            "label": "AppD Password", 
                            "help": "The password you use to login to AppDynamics", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "field": "account_name", 
                            "label": "AppD Account Name", 
                            "help": "For most on-premise deployments this will be the default value of 'customer1'.   For SaaS deployments this will be your account name. ", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "defaultValue": "customer1"
                        }, 
                        {
                            "required": true, 
                            "field": "duration", 
                            "label": "Time Duration (in minutes)", 
                            "help": "The time period (in minutes) that you wish to retrieve data for.  e.g.:  5 = retrieve data for the past 5 minutes.  ", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "defaultValue": "5"
                        }, 
                        {
                            "options": {
                                "items": [
                                    {
                                        "label": "Application Performance", 
                                        "value": "apm"
                                    }, 
                                    {
                                        "label": "End User Experience", 
                                        "value": "euem"
                                    }, 
                                    {
                                        "label": "Application Events", 
                                        "value": "events"
                                    }, 
                                    {
                                        "label": "Health Rule Violations", 
                                        "value": "violations"
                                    }, 
                                    {
                                        "label": "Infrastructure Performance", 
                                        "value": "infra"
                                    }, 
                                    {
                                        "label": "Business Transactions", 
                                        "value": "bt"
                                    }
                                ], 
                                "delimiter": "~"
                            }, 
                            "required": true, 
                            "field": "metrics_to_collect", 
                            "label": "Metrics to Collect", 
                            "help": "In large environments, metrics collected from Infrastructure Performance and Business Transactions can result in large amounts of data. Use with caution based on the size of  your environment.", 
                            "type": "multipleSelect", 
                            "defaultValue": "apm~euem~events~violations"
                        }, 
                        {
                            "required": false, 
                            "field": "app_name", 
                            "label": "Application Name(s) (optional)", 
                            "help": "Blank retrieves ALL applications.  Each application is 1 API call per metric set (4 sets * 50 apps = 200 calls). DO NOT LEAVE THIS BLANK IF YOU HAVE 50+ APPS as the controller could be overwhelmed.", 
                            "type": "text", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "AppDynamics Summary"
                }
            ], 
            "title": "Inputs"
        }
    }, 
    "meta": {
        "name": "Splunk_TA_AppDynamics", 
        "version": "1.5.4", 
        "apiVersion": "3.0.0", 
        "displayName": "Splunk Add-on for AppDynamics", 
        "restRoot": "Splunk_TA_AppDynamics"
    }
}